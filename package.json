{
  "name": "ns-flip",
  "description": "build and use updatable templates: can be reapplied to code losing custom changes to files",
  "version": "1.3.0",
  "author": "YizYah @YizYah",
  "bin": {
    "ns": "./bin/run"
  },
  "keywords": [
    "template",
    "generator",
    "automated",
    "update",
    "cli",
    "custom",
    "customized",
    "handlebars",
    "versions",
    "versioning"
  ],
  "bugs": "https://github.com/NoStackApp/ns-flip/issues",
  "dependencies": {
    "@mrmlnc/readdir-enhanced": "^2.2.1",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "arr-diff": "^4.0.0",
    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "inflection": "^1.12.0",
    "js-yaml": "^3.14.0",
    "jsonfile": "^6.0.1",
    "just-handlebars-helpers": "^1.0.18",
    "listr": "^0.14.3",
    "merge-anything": "^3.0.6",
    "pluralize": "^8.0.0",
    "prompts": "^2.3.2",
    "read-pkg": "^5.2.0",
    "traverse": "^0.6.6",
    "walkdir": "^0.4.1",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/resources",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/NoStackApp/ns-flip",
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ns",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/NoStackApp/ns-flip",
  "scripts": {
    "lintfix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "eslint . --ext .ts --config .eslintrc",
    "oldtest": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
